<div id="channel_function_params">
    
           
    <div class="cfg_relay_time1" id="cfg_relay_time1" data-paramname="param1">
       <span>{% trans %}Full opening time{% endtrans %}</span>
       <div class="btn-group" role="group" aria-label="...">
			<input type="number" step="0.1" id="iodevice_channel_type_sec1" name="_iodevice_channel_type_sec1" value="{{ opening_time }}">
       </div>
       <span>{% trans %}sec.{% endtrans %}</span>
    </div>
    
    <div class="cfg_relay_time2" id="cfg_relay_time2" data-paramname="param3">
       <span>{% trans %}Full closing time{% endtrans %}</span>
       <div class="btn-group" role="group" aria-label="...">
			<input type="number" step="0.1" id="iodevice_channel_type_sec2" name="_iodevice_channel_type_sec2" value="{{ closing_time }}">
       </div>
       <span>{% trans %}sec.{% endtrans %}</span>
    </div>
    
    <div class="cfp_channel">
      <span>{% trans %}Open sensor{% endtrans %}</span>
    
      <div class="dropdown" id="subchannel_select" data-param_id="param2">
        <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenuChannels" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
          <span id="subchannel_name">{{ subchannel_selected }}</span>
        <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" aria-labelledby="dropdownMenuChannels">
           <li><a data-id="0">{% trans %}None{% endtrans %}</a></li>
           {% for sensor in channels %}
           <li><a data-id="{{ sensor.id }}">{{ sensor.name }}</a></li>
           {% endfor %}
        </ul>
     </div>
  </div>
</div>